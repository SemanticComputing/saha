		</dependency>
		<dependency>
			<groupId>org.geonames</groupId>
			<artifactId>geonames-ws-client</artifactId>
			<version>1.1.9</version>
		</dependency>
		<dependency>
		  <groupId>xml-apis</groupId>
		  <artifactId>xml-apis</artifactId>
		  <version>1.4.01</version>
		</dependency>
	</dependencies>

	<repositories>
		<repository>
			<id>Seco external</id>
			<name>Seco public packaged Java Packages</name>
			<url>http://www.seco.tkk.fi/software/maven/</url>
			<layout>default</layout>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>oracleReleases</id>
			<name>Oracle Released Java Packages</name>
			<url>http://download.oracle.com/maven</url>
			<layout>default</layout>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<!-- <repository> <id>jBoss</id> <name>jBoss Released Java Packages</name> 
			<url>http://repository.jboss.com/maven2/</url> <layout>default</layout> <snapshots> 
			<enabled>false</enabled> </snapshots> </repository> -->
